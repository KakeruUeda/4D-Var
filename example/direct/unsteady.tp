Base
{
    dim = 3
    outputDir = "straightTube"
    numOfOMP = 1
}

Grid
{
    type = "Structured"
    extractFluid = "ON"
    nNodesInCell = 8
    node = "input/straightTube/64x32x32/node.dat"
    cell = "input/straightTube/64x32x32/cell.dat"
    image = "input/straightTube/64x32x32/image.dat"
}

Boundary
{
    velocityDirichlet = "input/straightTube/64x32x32/velocityDirichletPoiseuille.dat"
    pressureDirichlet = "input/straightTube/64x32x32/pressureDirichlet.dat"
}

PysicalParameter
{   
    rho = 200
    mu = 1   
}

TimeParameter
{
    dt = 0.01
    timeMax = 1000
    pulsatileFlow = "ON"
    pulseBeginItr = 100
    T = 1.0
}

