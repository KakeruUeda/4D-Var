DarcyParameter
{
    alpha = 1e-1
    resistance = 1e2
}

Postprocess
{
    isSnapShot = "ON"
    nSnapShot = 10
    snapInterval = 5
    snapTimeBeginItr = 250
    nNodesInDataCell = 8
    origin = (1.0, 0.0, 0.0)
    nxData = (12, 12, 12)
    lxData = (1.0, 1.0, 1.0)
}

